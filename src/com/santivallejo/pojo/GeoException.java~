package com.santivallejo.pojo;

public class GeoException extends Exception {

	/**
	 *
	 */
	private static final long serialVersionUID = 1L;
	private int statusCode;
	private String msjException;

	public GeoException(int status, String msj) {
		super();

		this.setStatusCode(status);
		this.setMsjException(msj);
	}

	/**
	 * @return the statusCode
	 */
	public int getStatusCode() {
		return statusCode;
	}

	/**
	 * @param statusCode
	 *            the statusCode to set
	 */
	public void setStatusCode(int statusCode) {
		this.statusCode = statusCode;
	}

	/**
	 * @return the msjException
	 */
	public String getMsjException() {
		return msjException;
	}

	/**
	 * @param msjException
	 *            the msjException to set
	 */
	public void setMsjException(String msjException) {
		this.msjException = msjException;
	}

}
